@model IEnumerable<Student>

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Course Controller</title>
</head>
<body>
    <p> List file in Course Controller</p>  
    <table>
        <thead> 
            <tr>
                <th>Name</th>
                <th>Email</th>
                <th>Phone</th>
                <th>Confirm</th>
            </tr>
        </thead>
        <tbody>
            @foreach(var item in Model)
            {
                <tr>
                    <td> @item.Name </td>
                    <td> @item.Email </td>
                    <td> @item.Phone </td>
                    <td> @item.Confirm </td>
                </tr>
            }
        </tbody>
    </table>


</body>
</html>